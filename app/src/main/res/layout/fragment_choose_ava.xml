        android:layout_height="0dp"
        android:background="#00000000"
        android:cacheColorHint="#00000000"
        android:src="@mipmap/m3"
        android:tag="m3"
        app:layout_constraintBottom_toTopOf="@+id/btnMan6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnMan2"
        app:layout_constraintTop_toBottomOf="@+id/btnWoman6"
        app:layout_constraintWidth_percent="0.26" />

    <ImageButton
        android:id="@+id/btnMan2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00000000"
        android:cacheColorHint="#00000000"
        android:src="@mipmap/m2"
        android:tag="m2"
        app:layout_constraintBottom_toTopOf="@+id/btnMan5"
        app:layout_constraintEnd_toStartOf="@+id/btnMan3"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnMan1"
        app:layout_constraintTop_toBottomOf="@+id/btnWoman5"
        app:layout_constraintWidth_percent="0.26" />

    <ImageButton
        android:id="@+id/btnWoman4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00000000"
        android:cacheColorHint="#00000000"
        android:src="@mipmap/w4"
        android:tag="w4"
        app:layout_constraintBottom_toTopOf="@+id/btnMan1"
        app:layout_constraintEnd_toStartOf="@+id/btnWoman5"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnWoman1"
        app:layout_constraintWidth_percent="0.26" />

    <ImageButton
        android:id="@+id/btnWoman6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00000000"
        android:cacheColorHint="#00000000"
        android:src="@mipmap/w6"
        android:tag="w6"
        app:layout_constraintBottom_toTopOf="@+id/btnMan3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnWoman5"
        app:layout_constraintTop_toBottomOf="@+id/btnWoman3"
        app:layout_constraintWidth_percent="0.26" />

    <ImageButton
        android:id="@+id/btnWoman5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00000000"
        android:cacheColorHint="#00000000"
        android:src="@mipmap/w5"
        android:tag="w5"
        app:layout_constraintBottom_toTopOf="@+id/btnMan2"
        app:layout_constraintEnd_toStartOf="@+id/btnWoman6"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
