        android:tag="m5"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toStartOf="@+id/btnMan6"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnMan4"
        app:layout_constraintTop_toBottomOf="@+id/btnMan2"
        app:layout_constraintWidth_percent="0.26" />

    <ImageButton
        android:id="@+id/btnMan6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00000000"
        android:cacheColorHint="#00000000"
        android:src="@mipmap/m6"
        android:tag="m6"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnMan5"
        app:layout_constraintTop_toBottomOf="@+id/btnMan3"
        app:layout_constraintWidth_percent="0.26" />

    <ImageButton
        android:id="@+id/btnMan1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_invisible"
        android:src="@mipmap/m1"
        android:tag="m1"
        app:layout_constraintBottom_toTopOf="@+id/btnMan4"
        app:layout_constraintEnd_toStartOf="@+id/btnMan2"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
