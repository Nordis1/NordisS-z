        android:tag="w1"
        app:layout_constraintBottom_toTopOf="@+id/btnWoman4"
        app:layout_constraintEnd_toStartOf="@+id/btnWoman2"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.26" />

    <ImageButton
        android:id="@+id/btnWoman2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00000000"
        android:cacheColorHint="#00000000"
        android:src="@mipmap/w2"
        android:tag="w2"
        app:layout_constraintBottom_toTopOf="@+id/btnWoman5"
        app:layout_constraintEnd_toStartOf="@+id/btnWoman3"
